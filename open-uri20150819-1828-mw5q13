	<!--Floating배너:S-->
	


<script type="text/javascript">

	jQuery(function() {
		if( !TMCookieUtil.isExist(1, 'searchfloatAdv') ){
			try{
				var targetUrl ="http://ds.11st.co.kr/NetInsight/text/11st/11st_search/search@Floating"+getNitmusParam(false);
				var adFloatingAdvHTML = '';
				var floatingAdvcaller = new getJsonpData();
				floatingAdvcaller.setUrl(targetUrl);
				floatingAdvcaller.setCallbackName("dsFloatAdBack");
				floatingAdvcaller.init();
			}catch(e){}
		}
	});
	function dsFloatAdBack(data) {
		try {
			if (data === null || data === undefined) {
			}else if (data && typeof data == 'object') {
				if ("LURL1" in data && "IMG1" in data) {
					adFloatingAdvHTML = makeFloatingADHTML(data.LURL1, data.IMG1);
					if (adFloatingAdvHTML != null && "" != adFloatingAdvHTML.trim()) {
						jQuery('#floatingBannerAD').html(adFloatingAdvHTML);
						jQuery('#floatingBannerAD').css('top', '-100px');
						jQuery('#floatingBannerAD').css('visibility', 'visible');

					
						doCommonStat("adLayerPopup&area=totalSearch");
					
					}
				}
			}
		} catch (ignore) {
		}
	};

	function todayFloatingAdvClose(){
		TMCookieUtil.add(1,'searchfloatAdv');
		jQuery('#floatingBannerAD').css('visibility', 'hidden');
	}

	function makeFloatingADHTML(linkURL, imgURL) {
		var htmlArray = [];
		htmlArray.push("<div class='flot_ctschup' style='background: url(\"" + imgURL + "\") no-repeat 0px 0px;'>");
		htmlArray.push("<a href='"+linkURL+"' class='ctsch_cnt' target='_blank' onclick=\"stck('C', 'F01', (function(){return (new Date()).toYYYYMMDD() + 'EY09I';})());\">이벤트 바로가기<\/a>");
		htmlArray.push("<button class='ctsch_todayclose' type='button' onclick='javascript:todayFloatingAdvClose();'>오늘 그만보기<\/button>");
		htmlArray.push("<button class='ctsch_close' type='button' onclick='jQuery(\"#floatingBannerAD\").css(\"visibility\", \"hidden\");'>플로팅 닫기<\/button>");
		htmlArray.push("<\/div>");
		return htmlArray.join('');
	};

	function getNitmusParam(isOtherParam){
		var param =TMCookieUtil. getSubCookie(0,'nitmus');
		var result="";
		var appendFirstStr="";
		if (param !== ""){
			param = decodeURIComponent(param);
			appendFirstStr = isOtherParam ? "&" : "?";
			result = appendFirstStr+param+ "&noCache=" + (new Date()).getTime();
		}else{
			result = "?noCache=" + (new Date()).getTime();
		}

		return result;
	};

</script>
<!-- 2012-04-24 플롯팅배너 추가 -->
