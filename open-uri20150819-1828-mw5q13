<script type="text/javascript">
if (typeof window.Combine === "undefined") {
	window.CombineSearch = {};
}

CombineSearch.PowerLink = {
	baseRequestUrl: 'http://ext.apbox.nbp-corp.com/11st/adunit',
	dataSize: 0,
	content: '',
	requestData: function() {
		var me = this;
		
		try {
			jQuery.ajax({
				url: me.createRequestUrl(),
				dataType: "jsonp",
				scriptCharset: "utf-8",
				success: function(data) {
					me.contData = data.ads;
					me.dataSize = me.contData.length;
					
					if (me.dataSize > 0) {
						jQuery('#idPowerLink').show();
						
						for(var i=0; i< me.dataSize && i<6; i++ ) {
							var dataList = me.contData[i];
							var title = dataList.title;
							var desc = dataList.desc;
							var clickUrl = dataList.cUrl;
		
							title = title.replace('수박', '<em>수박<\/em>');
							desc = desc.replace('수박', '<em>수박<\/em>' );
		
							var $titleObj = jQuery('#idPowerLinkTitle'+i);
							$titleObj.attr('href', clickUrl);
							$titleObj.html(title);
		
							var $linkDscObj = jQuery('#idPowerLinkDesc'+i);
							$linkDscObj.attr('href', clickUrl);
							$linkDscObj.html(desc);
		
							var $linkUrlObj = jQuery('#idPowerLinkUrl'+i);
							$linkUrlObj.attr('href', clickUrl);
							$linkUrlObj.text(dataList.vUrl);
		
							jQuery('#idPowerLinkLi'+i).show();
						}
					}
				},
				error: function() {}
			});
		}catch(e) {}
	},
	createRequestUrl: function() {
		var me = this;
		var url = ['output=jsonp'];
		
		url.push('ch=11st.ch1');
		url.push('kwd1='+encodeURIComponent("수박"));
		url.push('cnt=6');
		url.push('ref='+encodeURIComponent(document.referrer));
		url.push('cat1='+encodeURIComponent("*통합검색"));
		url.push('cat2='+encodeURIComponent(""));
		url.push('cat3='+encodeURIComponent(""));
		url.push('cat4='+encodeURIComponent(""));
		
		return me.baseRequestUrl + '?' + url.join('&');
	}
};

jQuery(document).ready(function() {
	CombineSearch.PowerLink.requestData();
});
</script>

