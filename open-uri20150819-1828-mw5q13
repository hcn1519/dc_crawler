var $wrapper = jQuery("ul[id=totalSearchRecommendPrdUI]");
$wrapper.delegate("li", "mouseenter", function() {
			jQuery(this).addClass("selected").siblings().removeClass("selected");
}).find("li").eq(0).addClass("selected");

$wrapper.delegate("li", "focusin", function() {
	jQuery(this).addClass("selected").siblings().removeClass("selected");
}).find("li").eq(0).addClass("selected");

jQuery("#recommPrd_apply").click(function() {
	$(this).children("p").toggle('fast');
});

