	if(jQuery('#areaCategoryList').children().length>12){
		jQuery('#areaCategoryList li:gt(11)').hide();
	}

	var isCategoryExpend = false;

	jQuery(document).ready(function(){
		if(isCategoryExpend){
			SearchCategory.categoryExpand();
		}
	})
</script>

