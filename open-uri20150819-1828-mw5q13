<script>
function goDetailPage(contNo, contCd){
	var strContCd = contCd.substring(0,3);
	if( strContCd == '106' || strContCd == '108' ) { //lifeStyle, 도서웹진
		url = 'http://www.11st.co.kr/community/LifeStyleAction.tmall?method=getLifeStyleDetail&contNo='+contNo;
	} else {
		url = 'http://www.11st.co.kr/browsing/OpenContContentAction.tmall?method=getFashionTrendDetail&contNo='+contNo;
	}

	window.open(url, 'trendView');
}

function viewReviewDetail(contNo,minorSelCnYn){

	if(minorSelCnYn=='N') {
		if(true) {
			openLoginAdults(1);
			return;

		}else if(false) {
			url = _GNB_CONTEXT_PATH_ + "/checkAdult.page?isButton=N";
            window.open(url, 'PreView', "width=418, height=181, scrollbars=no, status=no");
            return;
		}
	}

	window.open("http://www.11st.co.kr/browsing/OpenContReviewAction.tmall?method=getMyProfessionalReviewDetail&contNo="+contNo);
}

function goContentSearch(){
	var keyword = jQuery("input[name=kwd]").val();
	window.open("http://search.11st.co.kr/ContentsSearchAction.tmall?method=getContentsSearch&kwd=" + encodeURIComponent(encodeURIComponent(keyword)));
}

function goOpenShopping(){
	var keyword = jQuery("input[name=kwd]").val();
	window.open("http://www.11st.co.kr/browsing/OpenContSearchAction.tmall?method=getOpenContSearch&keyword=" + encodeURIComponent(encodeURIComponent(keyword)));
}
</script>


