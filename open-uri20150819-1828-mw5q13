	function listingCtgrSelBoxClick(){
		if(document.getElementById("listing_ctgr_selList").style.display == "block"){
			document.getElementById("listing_ctgr_selList").style.display = "none";
		}else{
			document.getElementById("listing_ctgr_selList").style.display = "block";
		}
		doCommonStat('SRA02');
	}
</script>
 
