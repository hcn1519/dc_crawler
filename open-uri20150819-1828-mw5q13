	var partnerDataArr = partnerData.result;
	var hasDealPrd = partnerData.hasDealPrd;

	var filterHtml = "";
	var cnt = 0;
	
	jQuery(document).ready(function(){
	//1. 제휴사 필터설정
	jQuery.each(partnerDataArr,function(index, val){
		var dispObjNm = val.dispObjNm;
		var sellerNo = val.sellerNo;
		var sellerImg = val.sellerImg;
		var extraText = val.extraText;
		if(extraText != undefined && extraText.length > 7){
			extraText = extraText.substring(0,7);
		}
		
		if( sellerImg.indexOf(_UPLOAD_URL_) < 0 ){
			sellerImg = _UPLOAD_URL_ + sellerImg;
		}

		if(index % 5 == 0){
			filterHtml += "<ul id=\"ptnrBnnrUl"+cnt+"\" ";
			filterHtml += "name=\"ptnrBnnrUlname\" ";
			filterHtml += "style=\"display:" + (cnt==0) ? " block;\">" : "none;\">" ;
		}
		
		filterHtml += "<li name=\"ptnrBnnrLi\"><a href=\"#\" onclick=\"doCommonStat('SRZZ04');\" "
		
		//쇼킹딜 배너 추가
		if(index == 0 && hasDealPrd ) { 
			filterHtml += "class=\"partnerBnnrFilter\" name=\"dealBnnr\" value=\"deal\">"
		} else {
			filterHtml += "class=\"partnerBnnrFilter\" name=\"partnerBnnrFilter\" value=\""+sellerNo+"\">"
		}
		filterHtml += "<span style=\"background-image:url(\'"+sellerImg+"\');\">"+dispObjNm+"</span></a>";
		
		if(extraText != undefined && extraText != ''){
			filterHtml += "<div class=\"benefit_area\"><span>"+extraText+"</span></div>";
		}
		filterHtml += "</li>";
		
		if(index % 5 == 4 || partnerDataArr.length == index-1){
			filterHtml += "<\/ul>";
			cnt++;
		}
	});
		jQuery("#filterdivPartnerBnnr").html(filterHtml);

		if(partnerDataArr.length<6){
			jQuery("#partnerDivFilterNaviButton").attr("class","btnctr_pn");
			jQuery("#partnerDivFilterNaviButton").find("button[name=preButton]").attr("disabled", "true");
			jQuery("#partnerDivFilterNaviButton").find("button[name=nextButton]").attr("disabled", "true");
		}
	});
	function clickArrow(navi){
		jQuery(".partner_filter ul").each(function(idx, val){
			if(navi == "fwd"){
				if(jQuery(this).css("display")== "block"){
					jQuery('#ptnrBnnrUl' + (idx)).css("display", "none");
					if(partnerDataArr.length % 5 == 0){
						if( idx+1 == cnt){
							jQuery('#ptnrBnnrUl0').css("display", "block");
						}else{
							jQuery('#ptnrBnnrUl' + (idx + 1)).css("display", "block");
						}
					}else{
						if( idx == cnt){
							jQuery('#ptnrBnnrUl0').css("display", "block");
						}else{
							jQuery('#ptnrBnnrUl' + (idx + 1)).css("display", "block");
						}
					}
						return false;
				}
			}else if(navi == "back"){
				if(jQuery(this).css("display")== "block"){
					jQuery('#ptnrBnnrUl' + (idx)).css("display", "none");
					if(partnerDataArr.length % 5 == 0){
						if( idx == 0){
							jQuery('#ptnrBnnrUl' + (cnt - 1)).css("display", "block");
						}else{
							jQuery('#ptnrBnnrUl' + (idx - 1)).css("display", "block");
						}
					}else{
						if( idx == 0){
							jQuery('#ptnrBnnrUl' + cnt).css("display", "block");
						}else{
							jQuery('#ptnrBnnrUl' + (idx - 1)).css("display", "block");
						}
					}
						return false;
				}
			}
		});
	}
</script>


