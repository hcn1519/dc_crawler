<script type="text/javascript">
	//My history 최근 검색어 삽입
	jQuery(function(){
		var keyword = "수박";
		jQuery.ajax({
			url: "http://www.11st.co.kr/browsing/MyHistoryAjaxAction.tmall?method=insertKeywordData&keyword=" + keyword ,
			dataType : 'jsonp',
			scriptCharset : 'UTF-8',
			jsonp : false,
			timeout: 2000
		});
	});


//성별, 연령별 검색로그
jQuery(function(){
	try {
		var segValue = TMCookieUtil.getSubCookie(0, "TM_SGMT");
		var codeRemake = (parseInt(segValue, 16).toString(10) ^ 24951736);
		var resultVal = parseInt(codeRemake, 2).toString(10);
		if( resultVal.length > 0 && resultVal.length <= 3 ) {

			var val1 = resultVal.substring(0,1);
			var val2 = resultVal.substring(1,3);

			var sexCd = "";
			var ageCd = "";

			//남자
			if( val1 == 1 ) {
				sexCd = "M";

				if(val2 <= 9){
					ageCd = "0";
				} else if( val2 >= 10 && val2 <= 19 ) {
					ageCd = "1";
				} else if( val2 >= 20 && val2 <= 29 ) {
					ageCd = "2";
				} else if( val2 >= 28 && val2 <= 39 ) {
					ageCd = "3";
				} else if( val2 >= 33 && val2 <= 49 ) {
					ageCd = "4";
				} else if( val2 >= 37 && val2 <= 59 ) {
					ageCd = "5";
				}

			//여자
			} else if ( val1 == 2 ) {
				sexCd = "F";

				if(val2 <= 9){
					ageCd = "0";
				} else if( val2 >= 10 && val2 <= 19 ) {
					ageCd = "1";
				} else if( val2 >= 20 && val2 <= 29 ) {
					ageCd = "2";
				} else if( val2 >= 28 && val2 <= 39 ) {
					ageCd = "3";
				} else if( val2 >= 33 && val2 <= 49 ) {
					ageCd = "4";
				} else if( val2 >= 37 && val2 <= 59 ) {
					ageCd = "5";
				}
			}

			var segmentTrcImg = new Image();
			segmentTrcImg.src = 'http://st.11st.co.kr/c.st?sexCd=' + sexCd + '&ageCd=' + ageCd + '&kwd=수박' + '&noCache='+(new Date()).getTime();
		}
	} catch(e) {}
});
</script>
 
	</div>
</div>
