<script type="text/javascript">
	var BestContents = function(data){
		this.contId = data.contId;
		this.title = data.title;
		this.summary = data.summary;
		this.image = data.image;
		this.movieYn = data.movieYn;
		this.sourceType = data.sourceType;
		this.sourceName = data.sourceName;
		this.url = data.url;
	};
	var currBestContentIdx = 0;
	var bestContentArr = new Array();

	var contJson = {contId : "BLOG_201506_4915854"
				,title : "맛있는 수박고르는법과 수박보관방법"
				,summary : "맛있는 수박고르는법과 수박보관방법                      요즘 너무 맛있는 제절과일 수박! 당도가 상당히 높아요!!        그런데 이 맛있는 수박 잘 ..."
				,image : "http://i.011st.com/ex_cmt/R/140x140/1/85/0/0/src/http://postfiles13.naver.net/20150625_44/jh2326y_1435234889997cHi79_JPEG/SAM_4993.JPG?type=w1"
				,movieYn : "N"
				,sourceType : "네이버"
				,sourceName : "Jh무지개엄마의 감성육아♡"
				,url : "http://jh2326y.blog.me/220401568005"
				};
	bestContentArr.push(new BestContents(contJson));

	var contJson = {contId : "review_36755358"
				,title : "만족한 수박"
				,summary : "두번째 인데 완전만족입니다크기가 크고 당도가 역시 좋습니다계속 여기서 구매할예정입니다"
				,image : "http://i.011st.com/ex_cmt/R/140x140/1/85/0/0/src/http://i.011st.com/t/300_10/ak/8/3/5/9/1/5/1267835915_B_V126.jpg"
				,movieYn : "N"
